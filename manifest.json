{
	"version": "0.1.1",
	"name": "Lil Nouns Auction Tracker",
	"description": "Stay up-to-date on current and recent Lil Nouns auctions, without interrupting your workflow.",
	"manifest_version": 3,
	"background": {
		"service_worker": "service-worker.js"
	},
	"action": {
		"default_title": "Lil Nouns Auction Tracker"
	},
	"permissions": ["notifications"],
	"icons": {
		"16": "./icons/icon-16.png",
		"32": "./icons/icon-32.png",
		"48": "./icons/icon-48.png",
		"128": "./icons/icon-128.png"
	}
}
